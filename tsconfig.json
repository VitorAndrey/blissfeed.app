{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@screens/*": [
        "./src/screens/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@layout*": [
        "./src/components/layout/*"
      ],
      "@ui*": [
        "./src/components/ui/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
    }
  }
}